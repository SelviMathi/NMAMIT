#include<stdio.h>
#include<string.h>
int main()
{
 int choice;
 printf("Enter the number of choice to perform :");
 scanf("%d",&choice);
 switch(choice)
 {
  case 1:
      printf("You selected length of a string");
      break;
  case 2:
      printf("You selected rev of a string");
    break;
  case 3:
      printf("You selected copy of a string 2 to string 1");
    break;
  case 4:
      printf("You selected compare of 2 strings");
    break;
  case 5:
      printf("You selected concate of a string");
    break;
 default:
    printf("Enter from 1 to 5");
 }
 
  
 
    return 0;
}
