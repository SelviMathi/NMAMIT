
#include<stdio.h>
void main()
{
int noe;
scanf("%d ",&noe);
int arr[noe];
for(int count=0;count<noe;count++)
{
    scanf("%d",&arr[count]);
}
int max=arr[0];
for(int count=0;count<noe;count++)
{
   if(max<arr[count])
    max=arr[count];
}
int min=arr[0];
for(int count=0;count<noe;count++)
{
   if(min>arr[count])
    min=arr[count];
}
printf("Max:%d\nMin:%d",max,min);

}
