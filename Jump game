#include<stdio.h>
#include<stdbool.h>
bool jump(int*,int);
void main()
{
int size;
scanf("%d",&size);
int arr[size];
for(int i=0;i<size;i++)
scanf("%d",&arr[i]);
 printf("\nCan reach the last index: %s\n", jump(arr,size) ? "true" : "false");
}
bool jump(int* arr,int size)
{
    int max=0;
for(int i=0;i<size;i++)
{
    if(max<arr[i])
       return false;
    int max1=max+arr[i];
    if(max<max1)
        max=max1;
}
return true;
}
